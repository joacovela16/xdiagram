{
  "name": "x-diagram",
  "version": "0.1.30",
  "license": "MIT",
  "type": "module",
  "module": "./dist/x-diagram-es.js",
  "main": "./dist/x-diagram-cjs.js",
  "types": "./index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "decl": "tsc -t es5 --emitDeclarationOnly --declaration --outDir ./dist/types ./src/modules/core.ts ./src/modules/plugins.ts ./src/modules/themes.ts ./src/modules/components.ts ./src/modules/renderers.ts",
    "clean": "rimraf components && rimraf plugins && rimraf themes&& rimraf dist && rimraf index.d.ts",
    "prepublish": "npm run build",
    "postpublish": "npm run clean"
  },
  "dependencies": {
    "paper": "0.12.15"
  },
  "devDependencies": {
    "rimraf": "3.0.2",
    "tslib": "2.3.1",
    "typescript": "4.6.2",
    "vite": "2.8.6",
    "zx": "^5.2.0"
  },
  "files": [
    "index.d.ts",
    "dist/**",
    "plugins/**",
    "themes/**",
    "components/**",
    "renderers/**",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/x-core-es.js",
      "require": "./dist/x-core-cjs.js"
    },
    "./plugins": {
      "types": "./plugins/index.d.ts",
      "import": "./dist/x-plugins-es.js",
      "require": "./dist/x-plugins-cjs.js"
    },
    "./themes": {
      "types": "./themes/index.d.ts",
      "import": "./dist/x-themes-es.js",
      "require": "./dist/x-themes-cjs.js"
    },
    "./components": {
      "types": "./components/index.d.ts",
      "import": "./dist/x-components-es.js",
      "require": "./dist/x-components-cjs.js"
    },
    "./renderers": {
      "types": "./renderers/index.d.ts",
      "import": "./dist/x-renderers-es.js",
      "require": "./dist/x-renderers-cjs.js"
    }
  },
  "keywords": [
    "paperjs",
    "diagram",
    "graph",
    "draw"
  ]
}
